ions","resultId":"dWLxT0zRnDFbwTKZ","rangeL":32,"rangeH":35},{"_id":"3IU3ViICIh3Xos5z","type":2,"text":"Herbalist","range":[36,42],"drawn":false,"flags":{},"img":"modules/wfrp4e-core/art/careers/herbalist.webp","collection":"wfrp4e-core.career-descriptions","resultId":"MYzcmrUrSKLmshwu","rangeL":36,"rangeH":42},{"_id":"Ggz3XWX4ME4d2HNZ","type":2,"text":"Hunter","range":[43,52],"drawn":false,"flags":{},"img":"modules/wfrp4e-core/art/careers/hunter.webp","collection":"wfrp4e-core.career-descriptions","resultId":"2RxWvPZCKW2ShY5a","rangeL":43,"rangeH":52},{"_id":"4AF6UVM1bhsRwAsW","type":2,"text":"Mystic","range":[53,57],"drawn":false,"flags":{},"img":"modules/wfrp4e-core/art/careers/mystic.webp","collection":"wfrp4e-core.career-descriptions","resultId":"M9NiEgE7qaCXmskM","rangeL":53,"rangeH":57},{"_id":"WljDk4m2ZKyZzTcu","type":2,"text":"Scout","range":[58,68],"drawn":false,"flags":{},"img":"modules/wfrp4e-core/art/careers/scout.webp","collection":"wfrp4e-core.career-descriptions","resultId":"Xl9yyv67BB8a8XFo","rangeL":58,"rangeH":68},{"_id":"gL1s8uL7WLzGtL48","type":2,"text":"Bounty Hunter","range":[69,70],"drawn":false,"flags":{},"img":"modules/wfrp4e-core/art/careers/bounty-hunter.webp","collection":"wfrp4e-core.career-descriptions","resultId":"bsQN8B1mtZhl2WRu","rangeL":69,"rangeH":70},{"_id":"tIo1KDo8XCWpaPwk","type":2,"text":"Entertainer","range":[71,75],"drawn":false,"flags":{},"img":"modules/wfrp4e-core/art/careers/entertainer.webp","collection":"wfrp4e-core.career-descriptions","resultId":"b96g5MZr3rk5zYAo","rangeL":71,"rangeH":75},{"_id":"7ddUwPEd12BKK4CV","type":2,"text":"Messenger","range":[76,78],"drawn":false,"flags":{},"img":"modules/wfrp4e-core/art/careers/messenger.webp","collection":"wfrp4e-core.career-descriptions","resultId":"8pk2uCvAMUAXWbcP","rangeL":76,"rangeH":78},{"_id":"qVmkuwZGKla3Kc26","type":2,"text":"Wrecker","range":[79,79],"drawn":false,"flags":{},"img":"modules/wfrp4e-core/art/careers/wrecker.webp","collection":"wfrp4e-core.career-desc