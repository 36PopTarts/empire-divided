angeL":81,"rangeH":85},{"_id":"BqhzEwmD2WS025Vn","type":2,"text":"Dislocated Knee","range":[86,90],"drawn":false,"flags":{},"img":"modules/wfrp4e-core/icons/injuries/tornleg.png","collection":"wfrp4e-core.criticals","resultId":"8GrHGyhYlbdxqluu","weight":null,"rangeL":86,"rangeH":90},{"_id":"0Ax3V9KZqAf7OLw0","type":2,"text":"Crushed Foot","range":[91,93],"drawn":false,"flags":{},"img":"modules/wfrp4e-core/icons/injuries/tornleg.png","collection":"wfrp4e-core.criticals","resultId":"NPhLsxsUV8wwdKWj","weight":null,"rangeL":91,"rangeH":93},{"_id":"nzsaeFQ1rn0ddBE6","type":2,"text":"Severed Foot","range":[94,96],"drawn":false,"flags":{},"img":"modules/wfrp4e-core/icons/injuries/tornleg.png","collection":"wfrp4e-core.criticals","resultId":"BzIaW1w6eTUFWgkk","weight":null,"rangeL":94,"rangeH":96},{"_id":"09wdXAX0HkzssCzs","type":2,"text":"Cut Tendon","range":[97,99],"drawn":false,"flags":{},"img":"modules/wfrp4e-core/icons/injuries/tornleg.png","collection":"wfrp4e-core.criticals","resultId":"9s7eE2U30R774sxO","weight":null,"rangeL":97,"rangeH":99},{"_id":"ECAiMOR7QpY1tIJJ","type":2,"text":"Shattered Pelvis","range":[100,100],"drawn":false,"flags":{},"img":"modules/wfrp4e-core/icons/injuries/tornleg.png","collection":"wfrp4e-core.criticals","resultId":"F9aFPBvoCdxg8oFJ","weight":null,"rangeL":100,"rangeH":100}],"formula":"1d100","replacement":true,"displayRoll":true,"folder":"F0YO0Ei20tEwOqGR","sort":2800000,"permission":{"default":0,"EXWjgsS8YJlDvMy7":3,"xeHJ5GIxgxZvN0Tc":3},"flags":{"wfrp4e":{"key":"critleg"},"core":{"sourceId":"Compendium.wfrp4e-core.tables.j2joGAVBNJgS1G1g"},"wfrp4e-core":{"initialization-folder":"Critical Hit Tables","sort":400000}}}
