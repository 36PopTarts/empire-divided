{"_id":"sbQ5mDvNZ7qQm8YY","name":"Fr'hough Mournbreath","type":"creature","img":"modules/wfrp4e-core/tokens/mournbreath.webp","data":{"characteristics":{"ws":{"type":"number","label":"Weapon Skill","abrev":"WS","initial":70,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":70,"bonus":7,"cost":25},"bs":{"type":"Number","label":"Ballistic Skill","abrev":"BS","initial":35,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":35,"bonus":3,"cost":25},"s":{"type":"Number","label":"Strength","abrev":"S","initial":120,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":120,"bonus":12,"cost":25},"t":{"type":"Number","label":"Toughness","abrev":"T","initial":150,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":150,"bonus":15,"cost":25},"i":{"type":"Number","label":"Initiative","abrev":"I","initial":50,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":50,"bonus":5,"cost":25},"ag":{"type":"Number","label":"Agility","abrev":"Ag","initial":20,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":20,"bonus":2,"cost":25},"dex":{"type":"Number","label":"Dexterity","abrev":"Dex","initial":30,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":30,"bonus":3,"cost":25},"int":{"type":"Number","label":"Intellegence","abrev":"Int","initial":85,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":85,"bonus":8,"cost":25},"wp":{"type":"Number","label":"Willpower","abrev":"WP","initial":120,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":120,"bonus":12,"cost":25},"fel":{"type":"Number","label":"Fellowship","abrev":"Fel","initial":50,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":50,"bonus":5,"cost":25}},"status":{"wounds":{"type":"Number","label":"Wounds","abrev":"W","value":108,"max":108},"advantage":{"type":"Number","label":"Advantage","value":"0","max":10},"criticalWounds":{"type":"Number","label":"Critical Wounds","value":0,"max":15,"descriptions":""},"sin":{"type":"Numb