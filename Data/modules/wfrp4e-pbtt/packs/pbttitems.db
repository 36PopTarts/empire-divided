{"_id":"DeB1x5wRqEfbhBXh","name":"Hoartfrost Thews","type":"prayer","img":"modules/wfrp4e-core/icons/prayers/miracles.png","data":{"description":{"type":"String","label":"Description","value":"<p>Ulric cools your body, and frost forms on your flesh. You become immune to cold from any source.</p>"},"gmdescription":{"type":"String","label":"Description","value":""},"type":{"type":"String","label":"Type","value":"miracle"},"god":{"type":"String","label":"God","value":"Ulric"},"range":{"type":"String","label":"Range","value":"You"},"duration":{"type":"String","label":"Duration","value":"Fellowship Bonus hours"},"target":{"type":"String","label":"Target","value":"You","aoe":false},"damage":{"type":"String","label":"Damage","dice":"","value":"","addSL":false},"overcast":{"enabled":false,"label":"","valuePerOvercast":{"type":"","value":1,"SL":false,"additional":"","characteristic":"","bonus":false},"initial":{"type":"","value":1,"SL":false,"additional":"","characteristic":"","bonus":false}}},"effects":[],"folder":"PJOh3ILdqof5NVjd","sort":0,"permission":{"default":0,"nEodC92V8JiQPG6I":3},"flags":{"wfrp4e-pbtt":{"initialization-folder":"Ulrican Miracles","sort":0}}}
