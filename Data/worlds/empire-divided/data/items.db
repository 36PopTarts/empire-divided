r":"prefillDialog","effectApplication":"equipped","hide":false,"preventDuplicateEffects":false,"script":"let targets = Array.from(game.user.targets);\nif (!targets[0]) \n  return;\n\nlet targetActor = game.actors.get(targets[0].data.actorId);\n// Make sure the weapon used for the roll is the one with this effect.\nif (this.item._id === args.item._id) {\n  // Check if the target is prone\n  let proneFilter = targetActor.effects.find(e => e.label === \"Prone\");\n  if (!!proneFilter) {\n    // Add Impale and Precise if the target is prone, only for this roll\n    args.item.data.data.qualities.value.push({name: \"impale\"});\n    args.item.data.data.qualities.value.push({name: \"precise\"});\n    // Notify the users that this effect resolved\n    ui.notifications.notify(`<b>${this.effect.label}</b>: Target is Prone!`)\n  }\n}"}},"tint":null}],"folder":"Ps9ESRgckOFHPHvC","sort":600000,"permission":{"default":0,"hNHeJbKl47AOkoOO":3},"flags":{}}
