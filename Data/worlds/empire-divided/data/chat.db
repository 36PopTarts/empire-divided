ntDuplicateEffects":true}},"changes":[{"key":"data.characteristics.fel.initial","value":5,"mode":2}],"disabled":false,"duration":{"startTime":93966544350,"seconds":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"icon":"modules/wfrp4e-core/icons/talents/suave.png","label":"Suave","origin":"Actor.BNxW5IDuKy9Yws0J.OwnedItem.SGU51U1ZAv8gioDZ","tint":"","sourcename":"Suave","displayLabel":"Suave"},{"_id":"rcao8ow9N6L7IJJW","flags":{"wfrp4e":{"effectTrigger":"preRollTest","effectApplication":"actor","script":"if (args.testData.extra.skill && args.testData.extra.skill.name == \"Charm\" &&  (args.testData.roll <= 5 || args.testData.roll <= args.testData.target))\n{\n\nlet SL = Math.floor(args.testData.target / 10) - Math.floor(args.testData.roll / 10)\nlet ones = Number(args.testData.roll.toString().split(\"\").pop())\n\nif (ones > SL)\n   args.testData.extra.other.push(`<b>${this.effect.label}</b>: Can use unit place for base SL (+${args.testData.successBonus + args.testData.slBonus} bonus SL) if debating.`)\n}","preventDuplicateEffects":true}},"changes":[],"disabled":false,"duration":{"startTime":86820912006,"seconds":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"icon":"modules/wfrp4e-core/icons/talents/argumentative.png","label":"Argumentative","origin":"Actor.BNxW5IDuKy9Yws0J.OwnedItem.rVWxgtCHDcoCoONf","tint":"","sourcename":"Argumentative","displayLabel":"Argumentative"},{"_id":"gJoxnHjonGGojmHf","flags":{"wfrp4e":{"effectApplication":"actor","preventDuplicateEffects":false}},"changes":[{"key":"data.characteristics.int.initial","value":5,"mode":2}],"disabled":false,"duration":{"startTime":86821570800,"seconds":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"icon":"modules/wfrp4e-core/icons/talents/savvy.png","label":"Savvy","origin":"Actor.BNxW5IDuKy9Yws0J.OwnedItem.1ojkt2CJ1edk7Sfi","tint":"","sourcename":"Savvy","displayLabel":"Savvy"},{"_id":"wZns7vwnXGt2mdh1","flags":{"wfrp4e":{"effectApplication":"a