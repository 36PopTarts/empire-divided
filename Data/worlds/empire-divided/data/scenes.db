tEnc":{"type":"Boolean","label":"Count Enc","value":true},"source":{"type":"String","label":"Source"}},"sort":4200000,"flags":{"_sheetTab":"description","core":{"sourceId":"Compendium.wfrp4e-core.trappings.mCvZAj5F6hfUZhzR"}},"img":"modules/wfrp4e-core/icons/equipment/packs_and_containers/pouch.png","effects":[]},{"_id":"92avp64nRqgW4MSp","name":"Orc Leather Jerkin","type":"armour","data":{"description":{"type":"String","label":"Description","value":""},"gmdescription":{"type":"String","label":"Description","value":""},"quantity":{"type":"Number","label":"Quantity","value":1},"encumbrance":{"type":"Number","label":"Encumbrance","value":1},"price":{"type":"String","label":"Price","gc":"0","ss":"5","bp":"0"},"availability":{"type":"String","label":"Availability","value":"common"},"location":{"type":"Number","label":"Location","value":0},"worn":{"type":"Boolean","label":"Worn","value":true},"armorType":{"type":"String","label":"Armour Type","value":"softLeather"},"penalty":{"type":"String","label":"Penalty","value":""},"qualities":{"type":"String","label":"Qualities","value":""},"flaws":{"type":"String","label":"Flaws","value":"Ugly"},"special":{"type":"String","label":"Special","value":""},"maxAP":{"head":0,"lArm":0,"rArm":0,"lLeg":0,"rLeg":0,"body":1},"currentAP":{"head":-1,"lArm":-1,"rArm":-1,"lLeg":-1,"rLeg":-1,"body":-1},"source":{"type":"String","label":"Source"}},"sort":4300000,"flags":{"core":{"sourceId":"Compendium.wfrp4e-core.trappings.ipaDvYY3qS66o593"}},"img":"Images/Icons/Age%20of%20Reckoning%20Icons/Enemy/2427.png","effects":[]},{"_id":"NezKKEj61DcDweJA","name":"Melee (Basic)","type":"skill","data":{"description":{"type":"String","label":"Description","value":"<p>The Melee Skill represents specific training with a single type of close combat weaponry. Each Melee Specialization indicates training in using a specific class of weapon. If you don’t have the correct Specialization for a weapon you wish to use, refer to <span style=\"font-weight: bold;\">@JournalEntry[The Consumers' Guide] </span>for the correct weapon entry, and what penalties you will suffer. See <span style=\"font-weight: bold;\">Chapter 5: Rules </span>for more detail about combat and using the Melee Skill.</p>"},"gmdescription":{"type":"String","label":"Description","value":""},"advanced":{"value":"bsc","type":"String"},"grouped":{"value":"isSpec","type":"String"},"characteristic":{"type":"String","label":"Characteristic","value":"ws"},"advances":{"type":"Number","label":"Advances","value":5,"costModifier":0},"modifier":{"type":"Number","label":"Advances","value":0},"total":{"type":"Number","label":"Total"},"source":{"type":"String","label":"Source"}},"sort":4400000,"flags":{"_sheetTab":"details","core":{"sourceId":"Compendium.wfrp4e-core.skills.rOPmyLWa37e7s9v6"}},"img":"modules/wfrp4e-core/icons/skills/basic-skill-ws.png","effects":[]},{"_id":"7t1Tg1p9dOMQxf7i","name":"Orc Boots","type":"trapping","data":{"description":{"type":"String","label":"Description","value":""},"gmdescription":{"type":"String","label":"Description","value":""},"quantity":{"type":"Number","label":"Quantity","value":1},"encumbrance":{"type":"Number","label":"Encumbrance","value":1},"price":{"type":"String","label":"Price","gc":"0","ss":"2","bp":"6"},"availability":{"type":"String","label":"Availability","value":"common"},"location":{"type":"Number","label":"Location","value":0},"trappingType":{"type":"String","label":"Trapping Type","value":"clothingAccessories"},"worn":true,"spellIngredient":{"type":"String","value":""},"source":{"type":"String","label":"Source"}},"sort":4500000,"flags":{"_sheetTab":"details","core":{"sourceId":"Compendium.wfrp4e-core.trappings.nrIcHcULt5fjRKbL"}},"img":"Images/Icons/Age%20of%20Reckoning%20Icons/Enemy/2428.png","effects":[]},{"_id":"mBwRaW1bJqKzOOQD","name":"Orc Clothing","type":"trapping","data":{"description":{"type":"String","label":"Description","value":""},"gmdescription":{"type":"String","label":"Description","value":""},"quantity":{"type":"Number","label":"Quantity","value":1},"encumbrance":{"type":"Number","label":"Encumbrance","value":1},"price":{"type":"String","label":"Price","gc":"0","ss":"3","bp":"0"},"availability":{"type":"String","label":"Availability","value":"common"},"location":{"type":"Number","label":"Location","value":0},"trappingType":{"type":"String","label":"Trapping Type","value":"clothingAccessories"},"worn":true,"spellIngredient":{"type":"String","value":""},"source":{"type":"String","label":"Source"}},"sort":4600000,"flags":{"_sheetTab":"details","core":{"sourceId":"Compendium.wfrp4e-core.trappings.KWaFV7NPZqtgnL9Z"}},"img":"Images/Icons/Age%20of%20Reckoning%20Icons/Enemy/2271.png","effects":[]},{"_id":"nUdSBEHqcTRKWvm1","name":"Teef","type":"trapping","data":{"description":{"type":"String","label":"Description","value":"<dl>\n<dd>\n<blockquote>\"You, Snarkit, have got to be zogging kidding me. Nine teef, for this lot?\"</blockquote>\n<em>- Great Grizzler-Griff Makiki the Cunning</em></dd>\n</dl>\n<figure>\n<figcaption></figcaption>\n</figure>\n<p>\"Teef\"&nbsp;is the term used to describe the form of&nbsp;currency&nbsp;commonly used among&nbsp;Greenskin Tribes.</p>"},"gmdescription":{"type":"String","label":"Description","value":""},"quantity":{"type":"Number","label":"Quantity","value":1},"encumbrance":{"type":"Number","label":"Encumbrance","value":0},"price":{"type":"String","label":"Price","gc":"0","ss":"0","bp":"0"},"availability":{"type":"String","label":"Availability","value":"None"},"location":{"type":"Number","label":"Location","value":0},"trappingType":{"type":"String","label":"Trapping Type","value":"misc"},"worn":false,"spellIngredient":{"type":"String","value":""}},"sort":1000001,"flags":{"core":{"sourceId":"Item.XnPEQ7G3QjDA4V4x"}},"img":"Images/Icons/Age%20of%20Reckoning%20Icons/Own%20Icons/Misc/teef.png","effects":[]}],"data":{"status":{"advantage":{"value":0},"wounds":{"value":1}}},"flags":{"oppose":null}},"disposition":0,"displayBars":20,"bar1":{"attribute